				}, 
				{
					"CurrentState": "Greeting",
					"NextState": "Order",
					"Meaning": "-",
					"Style": "Polite",
					"Utterance": "Hello, good sir! What would you like?",
					"UtteranceId": "TTS-F85D6A6DDD3A90F46CD9BA5626BB27BF"
				}, 
				{
					"CurrentState": "Order",
					"NextState": "OrderResponse",
					"Meaning": "-",
					"Style": "Rude",
					"Utterance": "I want the greatest damn hot dog you have, right away!",
					"UtteranceId": "TTS-1DA46DC8060EF69F7943884BFFB1FAC1"
				}, 
				{
					"CurrentState": "Greeting",
					"NextState": "Order",
					"Meaning": "-",
					"Style": "-",
					"Utterance": "Hello, what would you like to order?",
					"UtteranceId": "TTS-B826C0714EF8C30BC6281B191BE9ACE3"
				}, 
				{
					"CurrentState": "Order",
					"NextState": "OrderResponse",
					"Meaning": "-",
					"Style": "Polite",
					"Utterance": "I would like a delicious hot dog with onion rings and ketchup, pretty please!",
					"UtteranceId": "TTS-79E2AD54D2D8DE6FF03A3ADDB6442A6D"
				}, 
				{
					"CurrentState": "OrderResponse",
					"NextState": "ReOrder",
					"Meaning": "-",
					"Style": "Rude",
					"Utterance": "Are you dumb? We only serve hamburgers here...",
					"UtteranceId": "TTS-B0B63EBD16A58E5A44BEC0A8CA7BBE84"
				}, 
				{
					"CurrentState": "Order",
					"NextState": "OrderResponse",
					"Meaning": "-",
					"Style": "-",
					"Utterance": "I would like a hot dog with chips.",
